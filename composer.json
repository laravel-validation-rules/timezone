{
    "name": "laravel-validation-rules/timezone",
    "description": "Validate that a given timezone is valid.",
    "type": "library",
    "keywords": [ "laravel", "validation", "timezone", "validator" ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Scott Robinson",
            "email": "scott@dor.ky",
            "homepage": "http://dor.ky"
        }
    ],
    "require": {
        "illuminate/validation": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/translation": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LVR\\Timezone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LVR\\Timezone\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
