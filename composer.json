{
    "name": "ssx/php-ssh-keypairs",
    "description": "Simple wrapper to generate RSA/SSH keypairs.",
    "type": "library",
    "keywords": [ "ssh", "keys", "rsa" ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Scott Wilcox",
            "email": "scott@dor.ky",
            "homepage": "http://dor.ky"
        }
    ],
    "require": {
        "phpseclib/phpseclib": "2.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "SSX\\SSH\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SSX\\SSH\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
